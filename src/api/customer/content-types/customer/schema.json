{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "password": {
      "type": "password",
      "required": true,
      "minLength": 8
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "USER"
      ]
    },
    "feature_flags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::feature-flag.feature-flag",
      "mappedBy": "users"
    },
    "mood_trackings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::mood-tracking.mood-tracking",
      "mappedBy": "users"
    }
  }
}
