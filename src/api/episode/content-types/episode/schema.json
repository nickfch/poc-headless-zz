{
  "kind": "collectionType",
  "collectionName": "episodes",
  "info": {
    "singularName": "episode",
    "pluralName": "episodes",
    "displayName": "Episode",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "time_duration": {
      "type": "integer",
      "max": 24,
      "min": 1
    },
    "rating": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 10
    },
    "location": {
      "type": "enumeration",
      "enum": [
        "IN_HOUSE",
        "OUT_SIDE",
        "PUBLIC_EVENT"
      ],
      "required": true,
      "default": "OUT_SIDE"
    },
    "notes": {
      "type": "text"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag",
      "mappedBy": "episode"
    },
    "episode_triggers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::episode-trigger.episode-trigger",
      "inversedBy": "episodes"
    },
    "panic_attack_symptoms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::panic-attack-symptom.panic-attack-symptom",
      "mappedBy": "episode"
    }
  }
}
